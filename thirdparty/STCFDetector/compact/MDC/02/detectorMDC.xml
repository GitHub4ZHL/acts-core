  <display>
    <vis name="WireVisField"    alpha="1.0" r="0.9" g="0.9"  b="0.9"   showDaughters="true"  visible="true"/>
    <vis name="WireVisSignal"   alpha="1.0" r="0.9" g="0.9"  b="0."   showDaughters="true"  visible="true"/>
    <vis name="WireVisFieldm"    alpha="1.0" r="0.9" g="0.1"  b="0.9"   showDaughters="true"  visible="true"/>
