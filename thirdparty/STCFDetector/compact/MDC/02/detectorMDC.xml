    <vis name="WireVisSignalp"   alpha="1.0" r="0.1" g="0.9"  b="0."   showDaughters="true"  visible="true"/>
    <vis name="LayerVis"       alpha="1.0" r="0." g="0.5"  b="0.5"   showDaughters="true"  visible="false"/>
    <vis name="SupportVis"       alpha="1.0" r="0.2" g="0.2"  b="0.4"   showDaughters="true"  visible="false"/>
