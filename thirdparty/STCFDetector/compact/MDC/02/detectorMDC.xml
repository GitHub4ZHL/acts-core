  <define> 
    <constant name="world_side"             value="10*m"/>
    <constant name="world_x"                value="world_side/2"/>
    <constant name="world_y"                value="world_side/2"/>
    <constant name="world_z"                value="world_side/2"/>
    
        <constant name="world_r"                value="860*mm"/>
	<constant name="initial_length"         value="100*cm"/>
	<constant name="rsense"         value="0.01*mm"/>
        <constant name="signal_clth"      value="0.0005*mm"/>
